<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.app.MainActivity$PlaceholderFragment"
    android:baselineAligned="false">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Timer"
            android:id="@+id/textView"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:textColor="#FFFFFF"
            android:singleLine="false"
            android:layout_weight="0.64" />

        <TextView
            android:layout_width="137dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Score"
            android:textColor="#FFFFFF"
            android:id="@+id/textView2"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:gravity="right"
            android:layout_weight="0.23" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1">


        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton"
            android:src="@drawable/target"
            android:background="@android:color/transparent"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton2"
            android:src="@drawable/target"
            android:background="@android:color/transparent"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton3"
            android:layout_alignBottom="@+id/imageButton2"
            android:layout_toRightOf="@+id/imageButton2"
            android:src="@drawable/target"
            android:background="@android:color/transparent"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton4"
            android:layout_alignBottom="@+id/imageButton3"
            android:layout_toRightOf="@+id/imageButton3"
            android:background="@android:color/transparent"
            android:src="@drawable/target"
            android:onClick="click" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton5"
            android:layout_below="@+id/imageButton"
            android:layout_alignRight="@+id/imageButton"
            android:layout_alignEnd="@+id/imageButton"
            android:background="@android:color/transparent"
            android:src="@drawable/target"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton6"
            android:src="@drawable/target"
            android:background="@android:color/transparent"
            android:layout_alignTop="@+id/imageButton5"
            android:layout_toRightOf="@+id/imageButton5"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton7"
            android:layout_alignBottom="@+id/imageButton6"
            android:layout_alignLeft="@+id/imageButton3"
            android:layout_alignStart="@+id/imageButton3"
            android:background="@android:color/transparent"
            android:src="@drawable/target"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton8"
            android:layout_alignBottom="@+id/imageButton7"
            android:layout_alignLeft="@+id/imageButton4"
            android:layout_alignStart="@+id/imageButton4"
            android:src="@drawable/target"
            android:background="@android:color/transparent"
            android:onClick="click" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton9"
            android:background="@android:color/transparent"
            android:src="@drawable/target"
            android:layout_below="@+id/imageButton5"
            android:layout_toLeftOf="@+id/imageButton6"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton10"
            android:src="@drawable/target"
            android:background="@android:color/transparent"
            android:layout_alignTop="@+id/imageButton9"
            android:layout_toRightOf="@+id/imageButton9"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton11"
            android:layout_alignBottom="@+id/imageButton10"
            android:layout_alignLeft="@+id/imageButton7"
            android:layout_alignStart="@+id/imageButton7"
            android:background="@android:color/transparent"
            android:src="@drawable/target"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton12"
            android:layout_alignBottom="@+id/imageButton11"
            android:layout_alignLeft="@+id/imageButton8"
            android:layout_alignStart="@+id/imageButton8"
            android:src="@drawable/target"
            android:background="@android:color/transparent"
            android:onClick="click" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1">
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton13"
            android:layout_below="@+id/imageButton9"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:src="@drawable/target"
            android:background="@android:color/transparent"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton14"
            android:layout_alignBottom="@+id/imageButton13"
            android:layout_alignLeft="@+id/imageButton10"
            android:layout_alignStart="@+id/imageButton10"
            android:src="@drawable/target"
            android:background="@android:color/transparent"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton15"
            android:layout_alignBottom="@+id/imageButton14"
            android:layout_alignLeft="@+id/imageButton11"
            android:layout_alignStart="@+id/imageButton11"
            android:background="@android:color/transparent"
            android:src="@drawable/target"
            android:onClick="click" />





        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton16"
            android:layout_alignBottom="@+id/imageButton15"
            android:layout_alignLeft="@+id/imageButton12"
            android:layout_alignStart="@+id/imageButton12"
            android:src="@drawable/target"
            android:background="@android:color/transparent"
            android:onClick="click" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1">
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton17"
            android:layout_below="@+id/imageButton13"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@android:color/transparent"
            android:src="@drawable/target"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton18"
            android:layout_alignBottom="@+id/imageButton17"
            android:layout_alignLeft="@+id/imageButton14"
            android:layout_alignStart="@+id/imageButton14"
            android:src="@drawable/target"
            android:background="@android:color/transparent"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton19"
            android:layout_alignBottom="@+id/imageButton18"
            android:layout_alignLeft="@+id/imageButton15"
            android:layout_alignStart="@+id/imageButton15"
            android:background="@android:color/transparent"
            android:src="@drawable/target"
            android:onClick="click" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton20"
            android:layout_alignBottom="@+id/imageButton19"
            android:layout_alignLeft="@+id/imageButton16"
            android:layout_alignStart="@+id/imageButton16"
            android:src="@drawable/target"
            android:background="@android:color/transparent"
            android:onClick="click" />
    </LinearLayout>

</LinearLayout>






